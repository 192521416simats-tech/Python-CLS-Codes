A = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]

B = [
    [9, 8, 7],
    [6, 5, 4],
    [3, 2, 1]
]

# Initialize result matrix with zeros
result = [[0, 0, 0],
          [0, 0, 0],
          [0, 0, 0]]

# Add the matrices
for i in range(len(A)):          # iterate through rows
    for j in range(len(A[0])):   # iterate through columns
        result[i][j] = A[i][j] + B[i][j]

# Display the result
print("Resultant Matrix:")
for r in result:
    print(r)
